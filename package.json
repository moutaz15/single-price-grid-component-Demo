{
  "name": "single-price-grid-component-master",
  "version": "1.0.0",
  "description": "Landing Page for price component",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/stylesheet.css -w",
    "devserver": " live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 10 versions' css/style.comp.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/stylesheet.css --output-style compressed",
    "build:css": "npm-run-all compile:sass prefix:css compress:css"
  },
  "author": "Moutaz Alaa",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.13",
    "concat": "^1.0.3",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0"
  }
}
